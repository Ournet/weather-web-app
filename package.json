{
  "name": "weather-web-app",
  "version": "0.1.0",
  "description": "Ournet weather-web-app",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib && rimraf ./types",
    "clean-test-code": "rimraf ./lib/**/*.test.js && rimraf ./types/**/*.test.d.ts"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ournet/weather-web-app.git"
  },
  "keywords": [
    "ournet",
    "weather",
    "weather-web-app"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ournet/weather-web-app/issues"
  },
  "homepage": "https://github.com/ournet/weather-web-app#readme",
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/i18n": "^0.8.3",
    "@types/node": "latest",
    "ava": "^0.23.0",
    "rimraf": "^2.6.2",
    "typescript": "latest"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "i18n": "^0.8.3",
    "ms": "^2.1.1",
    "ournet.logger": "^0.1.0",
    "preact": "^8.2.9"
  }
}
